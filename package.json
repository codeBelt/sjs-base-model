{
  "name": "sjs-base-model",
  "version": "1.8.2",
  "description": "BaseModel helps translate data to models",
  "keywords": [
    "base-model",
    "sjs-base-model",
    "structure",
    "structure-js",
    "typescript"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "post-commit": "git update-index -g"
    }
  },
  "scripts": {
    "---------- HELPERS -----------------------------------------": "",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "---------- DEVELOPMENT -------------------------------------": "",
    "prewatch": "npm run clean && npm run build:types",
    "watch": "rollup -cw",
    "---------- BUILD -------------------------------------------": "",
    "prebuild": "npm run type-check && npm run clean && npm run build:types",
    "build": "rollup -c",
    "---------- TESTING -----------------------------------------": "",
    "test": "npm run lint && npm run unit",
    "test:watch": "jest --watch",
    "lint": "tslint '{src,__tests__}/**/*.ts{,x}' --exclude 'src/typings.d.ts'",
    "unit": "jest",
    "coverage": "jest --coverage",
    "---------- PUBLISH -----------------------------------------": "",
    "release": "np",
    "------------------------------------------------------------": ""
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "dependencies": {},
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@babel/runtime": "7.4.5",
    "@types/jest": "24.0.15",
    "husky": "2.4.1",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "rimraf": "2.6.3",
    "rollup": "1.16.1",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.3",
    "rollup-plugin-uglify": "6.0.2",
    "ts-jest": "24.0.2",
    "tslib": "1.10.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.2"
  },
  "author": {
    "name": "Robert S. (codeBelt)",
    "email": "code@codebelt.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeBelt/sjs-base-model.git"
  },
  "bugs": {
    "url": "https://github.com/codeBelt/sjs-base-model/issues"
  },
  "homepage": "https://github.com/codeBelt/sjs-base-model#readme"
}
